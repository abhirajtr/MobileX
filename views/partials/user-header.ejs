<head>
    <meta charset="utf-8">
    <title>Evara - eCommerce HTML Template</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="/user-assets/imgs/theme/favicon.svg">
    <!-- Template CSS -->
    <link rel="stylesheet" href="/user-assets/css/maind134.css?v=3.4">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</head>

<body>
    <header class="header-area header-style-3 header-height-2">
        <div class="header-bottom header-bottom-bg-color sticky-bar">
            <div class="container">
                <div class="header-wrap header-space-between position-relative  main-nav">
                    <div class="logo logo-width-1 d-block">
                        <a href="index.html"><img src="/user-assets/imgs/theme/logo.svg" alt="logo"></a>
                    </div>
                    <div class="header-nav d-none d-lg-flex">
                        <div class="main-menu main-menu-padding-1 main-menu-lh-2 d-none d-lg-block">
                            <nav>
                                <ul>
                                    <li><a class="<%= locals.home? 'active': '' %>" href="/">Home</a>
                                    </li>
                                    <li>
                                        <a class="<%= locals.shop? 'active':'' %>" href="/shop">Shop</a>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <!-- <div class="hotline d-none d-lg-block">
                        <p><i class="fi-rs-headset"></i><span>Hotline</span> 1900 - 888 </p>
                    </div> -->

                    <% if (locals.user) { %>
                        <div class="header-action-right d-block ml-10">
                            <div class="header-action-2">
                                <div class="header-action-icon-2">
                                    <a href="/wishlist">
                                        <img alt="Evara" src="/user-assets/imgs/theme/icons/icon-heart.svg">
                                        <% if (locals.user && locals.count && locals.count.wishlist !==undefined) { %>
                                            <!-- Check if locals.user exists and locals.count exists with cart property defined -->
                                            <span class="pro-count blue">
                                                <%= locals.count.wishlist %>
                                            </span>
                                            <% } %>
                                    </a>
                                </div>
                                <div class="header-action-icon-2">
                                    <a class="mini-cart-icon" href="/cart">
                                        <img alt="Evara" src="/user-assets/imgs/theme/icons/icon-cart.svg">
                                        <% if (locals.user && locals.count && locals.count.cart !==undefined) { %>
                                            <!-- Check if locals.user exists and locals.count exists with cart property defined -->
                                            <span class="pro-count blue" id="cartCount">
                                                <%= locals.count.cart %>
                                            </span>
                                            <% } %>
                                    </a>
                                </div>

                                <div class="col-nav">

                                    <ul class="nav">
                                        <li class="dropdown nav-item">
                                            <a class="dropdown-toggle" data-bs-toggle="dropdown" href="#"
                                                id="dropdownAccount" aria-expanded="false"> <img
                                                    class="img-xs rounded-circle"
                                                    src="/admin-assets/imgs/people/avatar2.jpg" alt="User"></a>
                                            <div class="dropdown-menu dropdown-menu-end"
                                                aria-labelledby="dropdownAccount">
                                                <a class="dropdown-item" href="/profile"><i
                                                        class="material-icons md-perm_identity"></i>Profile</a>
                                                <!-- <a class="dropdown-item" href="#"><i class="material-icons md-settings"></i>Account Settings</a> -->
                                                <!-- <a class="dropdown-item" href="#"><i class="material-icons md-account_balance_wallet"></i>Wallet</a> -->
                                                <!-- <a class="dropdown-item" href="#"><i class="material-icons md-receipt"></i>Billing</a> -->
                                                <!-- <a class="dropdown-item" href="#"><i class="material-icons md-help_outline"></i>Help center</a> -->
                                                <div class="dropdown-divider"></div>
                                                <a class="dropdown-item text-danger" href="/logout"><i
                                                        class="material-icons md-exit_to_app"></i>Logout</a>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <% } else { %>
                            <a class="btn btn-sm btn-primary" href="/login">Login</a>
                            <% } %>
                </div>
            </div>
        </div>
    </header>